SmartHome{
	sensorTypes{
		TEMPERATURE, HUMIDITY
	}
	Location bedroom {
		Sensor temp1 TEMPERATURE ("/home/ardenia/projects/polytech/modeles/smart_home/custom_dataset/sensor1.csv")
	},
	Location kitchen {
		Sensor hum1 HUMIDITY ("/home/ardenia/projects/polytech/modeles/smart_home/custom_dataset/sensor2.csv")
	}
	rules{
		if "bedroom.temp1" < 20 for 20 min then "too cold"
	}
	
}